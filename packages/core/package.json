{
  "name": "@memstamp/core",
  "version": "0.1.0",
  "description": "Core library for memstamp â€” VCOT schema, hashing, Merkle trees",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dharma-Technologies/memstamp.git",
    "directory": "packages/core"
  },
  "author": "Dharma Technologies, Inc.",
  "license": "MIT",
  "keywords": [
    "vcot",
    "audit",
    "blockchain",
    "ai-agents",
    "merkle-tree",
    "verification"
  ]
}
