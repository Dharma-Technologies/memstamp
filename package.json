{
  "name": "memstamp",
  "version": "0.1.0",
  "private": true,
  "description": "Verifiable audit trails for AI agents using the VCOT open standard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dharma-Technologies/memstamp.git"
  },
  "author": "Dharma Technologies, Inc.",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules",
    "db:generate": "pnpm --filter @memstamp/api db:generate",
    "db:migrate": "pnpm --filter @memstamp/api db:migrate",
    "db:push": "pnpm --filter @memstamp/api db:push"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.90.0",
    "@types/node": "^20.11.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
